    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
    
        Employee e1 = new Employee("","",Double.parseDouble(jTextField3.getText()));
        
        JOptionPane.showMessageDialog(null, e1.salarioAnual());
    }                                        

